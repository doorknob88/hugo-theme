{
  "name": "parsel-hugo-theme",
  "version": "1.0.0",
  "description": "Hugo theme with Tailwind CSS and daisyUI",
  "scripts": {
    "dev": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css --watch",
    "build": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/main.css --minify"
  },
  "keywords": [
    "hugo",
    "theme",
    "tailwind",
    "daisyui"
  ],
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.5"
  }
}